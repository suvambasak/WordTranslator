    (PP
      (IN from)
      (NP
        (NP
          (DT a)
          (JJ traditional)
          (JJ CAPEX)
          (NN model)
          (PRN
            (-LRB- ()
            (VP
              (VB buy)
              (S
                (NP (DT the) (JJ dedicated) (NN hardware))
                (NP (CC and) (NNS depreciate))
                (NP (PRP it))
                (VP
                  (PP (IN over) (NP (DT a) (NN period)))
                  (PP (IN of) (NP (NN time))))))
            (-RRB- ))))
        (PP
          (TO to)
          (NP
            (NP (DT the) (JJ OPEX) (NN model))
            (PRN
              (-LRB- ()
